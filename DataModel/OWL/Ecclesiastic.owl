    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Baptism</IRI>
        <Literal datatypeIRI="&xsd;string">The Christian religious rite of sprinkling water on to a person’s forehead or of immersing them in water, symbolizing purification or regeneration and admission to the Christian Church. In many denominations, baptism is performed on young children and is accompanied by name-giving.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>BaptismalCandidate</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who is admitted to membership of a Christian church.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Bishop</IRI>
        <Literal datatypeIRI="&xsd;string">A senior member of the Christian clergy, usually in charge of a diocese and empowered to confer holy orders.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Chapter</IRI>
        <Literal datatypeIRI="&xsd;string">The governing body of a religious community</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Church</IRI>
        <Literal datatypeIRI="&xsd;string">A particular Christian organization with its own clergy, buildings, and distinctive doctrines.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Confessee</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who repents their sins and (in the Christian Church) seeks forgiveness from God.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Confessor</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a priest who hears confessions and gives absolution and spiritual counsel</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ConfirmationCandidate</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who is confirmed during the religious rite or sacrament of confirmation</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Congregation</IRI>
        <Literal datatypeIRI="&xsd;string">A group of people assembled for religious worship.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Convert</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who participates in a conversion of religious identity</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Council</IRI>
        <Literal datatypeIRI="&xsd;string">An advisory, deliberative, or administrative body of people formally constituted and meeting regularly.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>DisqualifiedFromEcclesiasticalOffice</IRI>
        <Literal datatypeIRI="&xsd;string">Activity where a person is disqualified ie declared ineligible for an ecclesiastical office.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>EcclesiasticalAdministrativeOffice</IRI>
        <Literal datatypeIRI="&xsd;string">An administrative position of authority or service in an ecclesiastical organisation.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>EcclesiasticalOffice</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A position of authority or service in an ecclesiastical organisation.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>EcclesiasticalOrganisation</IRI>
        <Literal datatypeIRI="&xsd;string">An organisation related to the Christian Church.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Godchild</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who is presented at baptism by a godparent who promises to take responsibility for their religious education.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Godparent</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who presents a person at baptism and promises to take responsibility for their religious education.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>HoldEcclesiasticalOffice</IRI>
        <Literal datatypeIRI="&xsd;string">The activity of a person holding an ecclesiastical office, i.e. a position of authority or service, typically one of a public nature.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>MembershipEcclesiasticalBody</IRI>
        <Literal datatypeIRI="&xsd;string">The activity of a person being a member of an ecclesiastical body.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Minister</IRI>
        <Literal datatypeIRI="&xsd;string">A member of the clergy, especially in the Presbyterian and Nonconformist Churches.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>MonasticOffice</IRI>
        <Literal datatypeIRI="&xsd;string">A position of authority or service in a monastic organisation.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Office</IRI>
        <Literal datatypeIRI="&xsd;string">A position of authority or service, typically one of a public nature</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Officiant</IRI>
        <Literal datatypeIRI="&xsd;string">The role of a person who performs a religious rite or presides over a religious service or ceremony.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
