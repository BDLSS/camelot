    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Conference</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A formal meeting of people with a shared interest, typically one that takes place over several days.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ConferenceItem</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A digital object that has been produced for, or as a result of, a conference.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ConferencePaper</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A paper presented at an academic conference for researchers to present and discuss their work.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>ConferencePoster</IRI>
        <Literal datatypeIRI="&xsd;string">A poster that has the purpose of presenting research information by an individual or representatives of research teams at a conference with an academic or professional focus. </Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>DiscussionPaper</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A paper that includes research but tend to be shorter and more selective in its approach, and more analytical and critical, than a research paper.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>DiscussionPaper</IRI>
        <Literal datatypeIRI="&xsd;anyURI">http://en.wikipedia.org/wiki/Essay</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>JournalArticle</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">An article that is published in a journal.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>Patent</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A patent protects new inventions and covers how things work, what they do, how they do it, what they are made of and how they are made.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>PatentApplication</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A request pending at a patent office for the grant of a patent for the invention described and claimed by that application.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>PostPrintArticle</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">An article that is in the final form post printing of the publication in which it is published.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>PrePrintArticle</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">An article in the form before it is published.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>PressArticle</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A written or recorded communication directed at members of the news media for the purpose of announcing something ostensibly newsworthy.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>ResearchPaper</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A paper that is intended to uncover a wide variety of sources on a given topic.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>ResearchPaper</IRI>
        <Literal datatypeIRI="&xsd;anyURI">http://en.wikipedia.org/wiki/Essay</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>TechnicalReport</IRI>
        <Literal datatypeIRI="&xsd;string">A report that describes the process, progress, or results of technical or scientific research or the state of a technical or scientific research problem. It might also include recommendations and conclusions of the research.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>WorkingPaper</IRI>
        <Literal datatypeIRI="&xsd;string">A preliminary scientific or technical paper. Often, authors will release working papers to share ideas about a topic or to elicit feedback before submitting to a peer reviewed conference or academic journal. Working papers are often the basis for related works, and may in themselves be cited by peer-review papers.</Literal>
    </AnnotationAssertion>
